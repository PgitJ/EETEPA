a=float(input("Digite o valor de A: "))
b=float(input("Digite o valor de B: "))
c=float(input("Digite o valor de C: "))

delta=(b*b)-(4*(a)*(c))

x1=((-b)+(delta ** (1/2)))/(2*a)
x2=((-b)-(delta ** (1/2)))/(2*a)

print("{}²+{}x+{}=0".format(a,b,c))
print("Suas raízes são: x1:{} x2:{}".format(x1,x2))
print("Delta: {}".format(delta))
